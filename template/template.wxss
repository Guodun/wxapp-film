.contain{
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    width: 100%;
    background-color: #eee;
    padding: 10rpx 15rpx;
}
.movies{
    width: 350rpx;
    background-color: #fff;
    border: 1px solid #fff;
    border-radius: 8rpx;
    margin-right: 10rpx;
    margin-bottom: 15rpx;
}
.movieImages{
    position: relative;
}
.movieImage{
    width: 350rpx;
}
.movieRating{
    box-sizing: border-box;
    position: absolute;
    bottom: 10rpx;
    left: 0;
    width: 100%;
    height: 50rpx;
    background-color:rgba(0, 0, 0, .65);
    color: white;
    text-align: right;
    line-height: 50rpx;
    padding-right: 20rpx;
}
.movieTips{
    display: flex;
    flex-direction: row;
}
.movieTip{
    border: 1px solid #ccc;
    border-radius: 5rpx;
    height:40rpx;
    margin:10rpx 5rpx;
    padding: 0 2rpx;
    color:#666;
}