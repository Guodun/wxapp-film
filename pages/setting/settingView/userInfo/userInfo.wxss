@import "../../setting.wxss";
.cellsItem::before{
    content: '';
    position: absolute;
    z-index: 99;
    width: 100%;
    height: 1px;
    transform: scaleY(0.5);
    background-color: #e5e5e5;
}
.placeHolder{
    position: absolute;
    top: 0rpx;
    right: 49rpx;
    display:inline-block;
    height:90rpx;
    line-height: 90rpx;
    color:#C8C8CD;
}
.editBtn{
    width:250rpx;
    height:78rpx;
    margin:0 auto;
    color:white;
    background-color:#47a86c;
    border-radius:34rpx;
    line-height:78rpx;
    text-align:center;
    margin-top: 30rpx;
}