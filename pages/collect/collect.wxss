.navTab{
    display: flex;
    flex-direction: row;
    width: 100%;
    height: 85rpx;
    border-bottom: 1rpx solid #eee;
}
.tabTitle{
    flex-grow: 1;
    text-align: center;
    line-height: 85rpx;
}
.switchNavTab{
    border-bottom: 4rpx solid green;
}
.movieList{
    display: flex;
    flex-direction: row;
    width: 700rpx;
    height: 400rpx;
    padding: 30rpx;
}
.movieImage{
    width: 300rpx;
    height: 420rpx;
    box-shadow: 10rpx 10rpx 30rpx rgba(0, 0, 0, 0.2);
}
.movieData{
    height: 420rpx;
    margin-left: 30rpx;
    color: gray;
    font-size: 29rpx;
}
.line{
    margin-bottom: 25rpx;
}
.title{
    font-size: 40rpx;
    color: black;
}
.personImage{
    width:620rpx;
    height:264rpx;
    border-radius:50%;
    background-position: center;
}
.personList{
    display:flex;
    flex-direction:row;
    width:700rpx;
    height:420rpx;
    margin:0 auto;
    margin-top:35rpx;
    border:1rpx solid #eee;
    border-radius:27rpx;
    box-shadow:10rpx 10rpx 30rpx rgba(0, 0, 0, 0.2);
    padding:45rpx;
    box-sizing:border-box;
}